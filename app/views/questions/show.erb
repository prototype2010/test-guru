<%= form_with model: @question, method: :delete do |form| %>
  <h1>Question from test <%= @question.test.title %></h1>
  <br/>
  <%=  @question.body %>
  <br/>
  <%= form.submit "Delete", data: { confirm: "Are you sure?" } %>
  <br/>
  <a href="<%= test_questions_path(@question.test) %>"> back to all questions</a>
<% end %>
